{
    "cleanUrls": true,
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "DENY"
                },
                {
                    "key": "X-XSS-Protection",
                    "value": "1; mode=block"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/confirmacao",
            "destination": "/confirmacao.html"
        },
        {
            "source": "/termos",
            "destination": "/termos.html"
        },
        {
            "source": "/privacidade",
            "destination": "/privacidade.html"
        },
        {
            "source": "/",
            "destination": "/index.html"
        }
    ]
}